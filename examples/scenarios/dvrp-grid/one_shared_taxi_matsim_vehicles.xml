<?xml version="1.0" encoding="UTF-8"?>

<vehicleDefinitions xmlns="http://www.matsim.org/files/dtd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
					xsi:schemaLocation="http://www.matsim.org/files/dtd http://www.matsim.org/files/dtd/vehicleDefinitions_v2.0.xsd">

	<vehicleType id="typeA">
		<description>Mode DRT A</description>
		<capacity seats="2"/>
	</vehicleType>
	<vehicleType id="typeB">
		<description>Mode DRT B</description>
		<capacity seats="2"/>
	</vehicleType>
	<vehicleType id="typeC">
		<description>Mode DRT C</description>
		<capacity seats="2"/>
	</vehicleType>

	<vehicle id="shared_taxi_one_A" type="typeA">
		<attributes>
			<attribute name="dvrpMode" class="java.lang.String">drt_A</attribute>
			<attribute name="startLink" class="java.lang.String">215</attribute>
			<attribute name="serviceBeginTime" class="java.lang.Double">0</attribute>
			<attribute name="serviceEndTime" class="java.lang.Double">8000</attribute>
		</attributes>
	</vehicle>
	<vehicle id="shared_taxi_one_B" type="typeB">
		<attributes>
			<attribute name="dvrpMode" class="java.lang.String">drt_B</attribute>
			<attribute name="startLink" class="java.lang.String">215</attribute>
			<attribute name="serviceBeginTime" class="java.lang.Double">0</attribute>
			<attribute name="serviceEndTime" class="java.lang.Double">8000</attribute>
		</attributes>
	</vehicle>
	<vehicle id="shared_taxi_one_C" type="typeC">
		<attributes>
			<attribute name="dvrpMode" class="java.lang.String">drt_C</attribute>
			<attribute name="startLink" class="java.lang.String">215</attribute>
			<attribute name="serviceBeginTime" class="java.lang.Double">0</attribute>
			<attribute name="serviceEndTime" class="java.lang.Double">8000</attribute>
		</attributes>
	</vehicle>

</vehicleDefinitions>