<?xml version="1.0" encoding="UTF-8"?>
<vehicleDefinitions
  xmlns="http://www.matsim.org/files/dtd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.matsim.org/files/dtd http://www.matsim.org/files/dtd/vehicleDefinitions_v2.0.xsd">

<vehicleType id="normal&amp;Car">
  <attributes>
    <attribute name="Attribute1" class="java.lang.String">abc</attribute>
    <attribute name="Attribute2" class="java.lang.Double">1.3</attribute>
    <attribute name="fuelConsumptionLitersPerMeter" class="java.lang.Double">0.23</attribute>
    <attribute name="freightCapacityInUnits" class="java.lang.Integer">200</attribute>
    <attribute name="accessTimeInSecondsPerPerson" class="java.lang.Double">23.23</attribute>
    <attribute name="egressTimeInSecondsPerPerson" class="java.lang.Double">42.42</attribute>
    <attribute name="doorOperationMode" class="java.lang.String">parallel</attribute>
    <attribute name="freightCapacityInUnits" class="java.lang.Double">200</attribute>

  </attributes>
  <description></description>
  <!-- capacity including driver -->
  <capacity>
    <seats persons="5"/>
    <standingRoom persons="20" />
    <freightCapacity>
      <volume cubicMeters="23.23"/>
      <weight tons="9.5"/>
    </freightCapacity>
  </capacity>
  <length meter="9.5" />
  <width meter="3" />
  <maximumVelocity meterPerSecond="42" />
  <engineInformation>
    <eiAttributes>
      <eiAttribute name="HbefaVehicleCategory" class="java.lang.String">pass. car</eiAttribute>
      <eiAttribute name="HbefaTechnology" class="java.lang.String">petrol</eiAttribute>
      <eiAttribute name="HbefaSizeClass" class="java.lang.String">&lt; 1,4L</eiAttribute>
      <eiAttribute name="HbefaEmissionsConcept" class="java.lang.String">EURO-5</eiAttribute>
    </eiAttributes>
  </engineInformation>
  <costsInformation>
    <costInformation fixedCostsPerDay="100.00" costsPerMeter="0.15" costsPerSecond="0.08"/>
  </costsInformation>
  <passengerCarEquivalents pce="2.0" />
  <networkMode networkMode = "pt" />
  <flowEfficiencyFactor factor="1.5" />
</vehicleType>

<vehicleType id="defaultValue&gt;Car">
  <attributes>
    <attribute name="Attribute1" class="java.lang.String">def</attribute>
    <attribute name="Attribute2" class="java.lang.Integer">2</attribute>
  </attributes>
</vehicleType>

<vehicle id="23" type="normal&amp;Car" />
<vehicle id="42" type="defaultValue&gt;Car" />
<vehicle id=" 42  23" type="defaultValue&gt;Car" />


</vehicleDefinitions>
